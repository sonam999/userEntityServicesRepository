<input type="text" ng-model="$ctrl.firstName">

<div layout="row" layout-wrap >
  <custom-input-box  flex="20" flex-offset="5" reflect-custom-input="$ctrl.reflectValue(keyString,value)" bind-data="$ctrl.firstName" bind-data-key="firstName" type='text' label="{{$ctrl.label}}" specific-attr='$ctrl.specificAttr'>
  </custom-input-box>


  <custom-input-box  flex="20" flex-offset="5" reflect-custom-input="$ctrl.reflectValue(keyString,value)" bind-data="$ctrl.checkinTime" bind-data-key="checkinTime" type='timePicker' label="{{$ctrl.datelabel}}">
  </custom-input-box>

  <custom-input-box  flex="20" flex-offset="5" reflect-custom-input="$ctrl.reflectValue(keyString,value)" bind-data="$ctrl.checkinDate" bind-data-key="checkinDate" type='datePicker' label="{{$ctrl.datelabel}}">
  </custom-input-box>

